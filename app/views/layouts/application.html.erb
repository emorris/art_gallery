<!DOCTYPE html>
<html>
<head>
  <title>ArtGallery</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <% flash.each do |key, message| %>
        <div class="alert alert-<%= key %>"> <%= message %> </div>
    <% end %>
    <%= yield %>
  </div>
</body>
</html>
